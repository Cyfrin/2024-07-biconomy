{
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "printWidth": 150,
        "tabWidth": 4,
        "proseWrap": "always",
        "useTabs": false,
        "singleQuote": false,
        "trailingComma": "all",
        "bracketSpacing": true,
        "plugins": ["prettier-plugin-solidity"]
      }
    }
  ]
}
